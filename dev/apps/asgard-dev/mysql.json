[{
  "id": "/asgard-dev/mysql",
  "cmd": null,
  "cpus": 0.8,
  "mem": 512,
  "disk": 0,
  "instances": 0,
  "constraints": [
    [
      "mesos",
      "LIKE",
      "slave"
    ],
    [
      "workload",
      "LIKE",
      "mysql-wordpress"
    ],
    [
      "owner",
      "LIKE",
      "asgard-dev"
    ]
  ],
  "acceptedResourceRoles": [
    "*"
  ],
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "mysql:5.7",
      "portMappings": [],
      "network": "HOST",
      "parameters": [
        {
          "key": "label",
          "value": "hollowman.appname=/asgard-dev/mysql"
        },
        {
          "key": "network",
          "value": "asgard"
        }
      ],
      "privileged": false,
      "forcePullImage": true
    },
    "volumes": [],
    "portMappings": []
  },
  "env": {
    "MYSQL_ROOT_PASSWORD": "secret"
  },
  "portDefinitions": [],
  "uris": [
    "file:///etc/docker.tar.bz2"
  ]
}]
